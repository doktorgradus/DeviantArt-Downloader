{
    "manifest_version": 2,

    "name": "DeviantArt Downloader",
    "description": "This extension allows the user to download images from DeviantArt in current page.",
    "version": "0.0.1",

    "browser_action": 
    {
        "default_icon": "icon.png",
        "default_popup": "menu.html",
        "default_title": "DeviantArt Downloader"
    },
    "options_page": "settings.html",
    "permissions": [
        "activeTab",
        "storage"
    ]
}